{
  "name": "Dev Warnings",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Warns when you are on a dev-version of a page. Intended for web developers.",
  "homepage_url": "https://matislepik.com/dev-warnings",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options/index.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "page action demo"
  },
  "web_accessible_resources": [
    "assets/overlay.png"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
